
<layout-elements>

  <element>
      <id>inventory</id>
      <type>LIST</type>
      <layout>
        <layout-type>INGAME</layout-type>
        <width>200</width>
        <height>400</height>
        <x>50</x>
        <y>50</y>
        <rows-max>5</rows-max>
        <cols-max>1</cols-max>
      </layout>
      <background>        
        <background-color>fpsColor</background-color>
        <top-color>Black</top-color>
      </background>
      <visible>false</visible>
      <listdata data="[#PLAYER_ITEMS(NAME,NAME),[sk,sk]]">
          <list-element>
            <id>invList</id>
            <type>TEXT</type>
            <text textcolor="fpsColor">No Data</text>
            <layout>
              <layout-type>INGAME</layout-type>
              <width>100</width>
              <height>20</height>
            </layout>
            <background>
              <background-color>transColor</background-color>
            </background>
            <position>CENTER</position>                          
          </list-element>
      </listdata>
      </element>
</layout-elements>
